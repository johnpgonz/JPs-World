import os
import getpass
import sys
import telnetlib

user = raw_input("Enter your telnet username: ")
password = getpass.getpass()

f = open("ioxr_list.txt")
line = f.readlines()
for devices in line:
    tn = telnetlib.Telnet(devices.rstrip("\n"))
    if password:
        tn.read_until("username:", 1) 
    tn.write(user + "\n")
    tn.read_until("Password:", 1)
    tn.write(password + "\n")
    tn.write("term len 0\n")
    tn.write("show ipv4 int brief | ex unass\n")
    tn.write("exit\n")
    print tn.read_all()
f.close()
